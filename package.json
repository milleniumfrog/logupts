{
  "name": "logupts",
  "version": "4.0.4",
  "description": "LogUpTs is designed as an extendable logging library for nodejs and the browser in typescript, but it is also usable in javascript. With LogUpTs you are able to use placeholders (\"{{day}}:{{month}}:{{year}}\" -> \"01.01.1970\" ) and transports (for example the file transport).",
  "main": "./dist/logupts.umd.js",
  "browser": "./dist/logupts.umd.js",
  "module": "./dist/logupts.js",
  "types": "./dist/logupts.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --opts ./mocha.opts",
    "coverage": "nyc npm test"
  },
  "keywords": [
    "logging",
    "loglevels",
    "nodejs",
    "browser",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/milleniumfrog/logupts.git"
  },
  "author": "Ren√© Schwarzinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milleniumfrog/logupts/issues"
  },
  "homepage": "https://github.com/milleniumfrog/logupts#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "rollup": "^1.11.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "typescript": "^3.6.2"
  }
}
